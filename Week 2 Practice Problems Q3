#include<iostream>
using namespace std;

int main(){
    int length, breadth, area, peri;
    cin>>length;
    cin>>breadth;
    
    area=length*breadth;
    peri=2*(length+breadth);
    
    if(area>peri)
    {
        cout<<"Area"<<endl;
        cout<<area;
        
    }
    else if(peri>area)
    {
        cout<<"Peri"<<endl;
        cout<<peri;
    }
    else {
        cout<<"Eq"<<endl;
        cout<<area;
    }
}
